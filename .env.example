# ReasonCare Environment Configuration

# Application Mode
# Set to 'true' for demo mode with mock data
# Set to 'false' for production mode with AWS services
REACT_APP_DEMO_MODE=true

# AWS Configuration
REACT_APP_AWS_REGION=us-east-1
REACT_APP_BEDROCK_REGION=us-east-1

# API Configuration (Production Mode)
REACT_APP_API_BASE_URL=https://api.reasoncare.io
REACT_APP_LAMBDA_ORCHESTRATOR_URL=https://your-lambda-url.lambda-url.us-east-1.on.aws/

# AWS Service Endpoints (Production Mode)
REACT_APP_DOCDB_ENDPOINT=reasoncare-docdb-cluster.cluster-xxxxx.docdb.us-east-1.amazonaws.com
REACT_APP_S3_BUCKET=reasoncare-production-assets
REACT_APP_KNOWLEDGE_BASE_ID=your-bedrock-knowledge-base-id

# Authentication (Production Mode)  
REACT_APP_AWS_ACCESS_TOKEN=your-jwt-token-here
REACT_APP_COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
REACT_APP_COGNITO_CLIENT_ID=your-cognito-client-id

# Feature Flags
REACT_APP_ENABLE_VOICE_INPUT=true
REACT_APP_ENABLE_AI_EXPLANATIONS=true
REACT_APP_ENABLE_GUIDELINE_UPDATES=true
REACT_APP_ENABLE_REAL_TIME_MONITORING=true

# Environment Identifier
REACT_APP_ENVIRONMENT=development

# Bedrock Model Configuration
REACT_APP_BEDROCK_CARDIOLOGIST_MODEL=anthropic.claude-3-sonnet-20240229-v1:0
REACT_APP_BEDROCK_REASONING_MODEL=anthropic.claude-3-sonnet-20240229-v1:0
REACT_APP_BEDROCK_SUMMARY_MODEL=anthropic.claude-3-sonnet-20240229-v1:0

# CloudWatch Monitoring
REACT_APP_CLOUDWATCH_LOG_GROUP=/aws/lambda/reasoncare-agent-orchestrator
REACT_APP_CLOUDWATCH_DASHBOARD_URL=https://console.aws.amazon.com/cloudwatch/

# Security Configuration
REACT_APP_KMS_KEY_ID=arn:aws:kms:us-east-1:123456789012:key/12345678-1234-1234-1234-123456789012
REACT_APP_WAF_WEB_ACL_ID=your-waf-web-acl-id

# Performance Monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ALERT_SNS_TOPIC_ARN=arn:aws:sns:us-east-1:123456789012:reasoncare-alerts